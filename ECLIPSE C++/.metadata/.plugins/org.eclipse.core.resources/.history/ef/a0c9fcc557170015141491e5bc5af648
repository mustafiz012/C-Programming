#include <bits/stdc++.h>
using namespace std;

int main() {
	for (int var = 0; var < 10; ++var) {
		printf("This is a test session: ");
		cout<<var<<endl;
		printf("I don't wanna be left behind\n");
	}
	return 0;
}
